'use strict';

var extend = function(obj, extension) {
    if (typeof obj === "object" && typeof extension === "object") {
        for (var i in extension) {
            if (extension.hasOwnProperty(i) &&
                !obj.hasOwnProperty(i)) {
                obj[i] = extension[i];
            }
        }
        return obj;
    }
};

var cat = {
  sound: 'meow',
  claws: true,
  lives: 9
};
var dog = {
  sound: 'woof',
  tail: "wags",
  likesWater: true
};
